def fizzbuzz(rangeA, rangeB):
    for i in range(rangeA, rangeB):
        if i % 3 == 0:
            if i % 5 == 0:
                print('FizzBuzz')
            else:
                print('Fizz')
        elif i % 5 == 0:
            print('Buzz')
        else:
            print(i)



fizzbuzz(1, 101)
