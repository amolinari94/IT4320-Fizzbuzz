def fizzbuzz(rangeA, rangeB):
    for i in range(rangeA, rangeB):
        if i % 3 == 0 and i % 5 == 0:
            print('FizzBuzz')
        if i % 3 == 0 and i % 5 != 0:
            print('Fizz')
        if i % 5 == 0 and i % 3 != 0:
            print('Buzz')
        if i % 3 != 0 and i % 5 != 0:
            print(i)

fizzbuzz(1, 101)
